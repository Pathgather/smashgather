{
  "name": "smashgather-server",
  "private": true,
  "description": "Relay server for Smashgather data",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm run build",
    "clean": "rm -rf dist/ public/assets/ && mkdir dist",
    "start": "node ./dist/production.js",
    "start-dev": "babel-node ./server/dev.js",
    "update-schema": "babel-node ./scripts/updateSchema.js",
    "create-db": "createdb smashgather; psql -d smashgather -f ./scripts/create_db.sql",
    "seed-db": "psql -d smashgather -f ./scripts/seed_db.sql",
    "build": "NODE_ENV=production webpack --config webpack.production.js --bail --profile -p && NODE_ENV=production babel -d ./dist ./server"
  },
  "dependencies": {
    "babel-cli": "6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "6.2.3",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "6.5.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-relay-plugin": "0.7.3",
    "classnames": "2.2.3",
    "compression": "^1.6.1",
    "express": "4.13.4",
    "express-graphql": "0.4.9",
    "express-jwt": "^3.3.0",
    "graphql": "0.4.17",
    "graphql-relay": "0.3.6",
    "moment": "^2.12.0",
    "pg": "^4.5.1",
    "react": "0.14.7",
    "react-dom": "0.14.7",
    "react-relay": "0.7.3",
    "react-transform-hmr": "^1.0.4",
    "webpack": "1.12.13",
    "webpack-dev-server": "1.14.1"
  }
}
